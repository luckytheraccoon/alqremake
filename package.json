{
  "name": "myprojectfe",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "start:dev": "npm-run-all --parallel css js:dev",
    "start:prod": "npm-run-all --parallel css js:prod",
    "js:dev": "npm-run-all concatjs babel build:dev",
    "js:prod": "npm-run-all concatjs babel build:prod",
    "css": "npm-run-all sass concatcss minifycss",
    "concatjs": "concat -o raw_resources/js/lib/merged-scripts.js raw_resources/js/src/imports.js raw_resources/js/src/index.js raw_resources/js/src/common-components.js raw_resources/js/src/main-render.js",
    "babel": "babel raw_resources/js/lib/merged-scripts.js --out-file raw_resources/js/lib/babelified-merged-scripts.js",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:prod": "webpack --env=prod --progress --profile --colors",
    "sass": "node-sass -o raw_resources/css/src/ raw_resources/css/src/",
    "concatcss": "cleancss --format beautify -o raw_resources/css/lib/merged-styles.css raw_resources/css/src/*.css",
    "minifycss": "cleancss -o css/main.css raw_resources/css/lib/merged-styles.css",
    "onchange:css": "onchange \"raw_resources/css/src/*.scss\" -- npm run css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^6.0.78",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^3.3.7",
    "clean-css": "^4.1.7",
    "clean-css-cli": "^4.1.6",
    "concat": "^1.0.3",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "eslint-plugin-react": "^7.1.0",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "onchange": "^3.2.1",
    "react": "~15.3.0",
    "react-addons-create-fragment": "~15.3.0",
    "react-addons-css-transition-group": "~15.3.0",
    "react-addons-update": "~15.3.0",
    "react-dom": "~15.3.0",
    "react-transition-group": "^1.2.0",
    "webpack": "^3.0.0"
  }
}
